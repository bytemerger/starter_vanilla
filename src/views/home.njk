<div class="container">
    <div class="header">
        <div class="header-index-name align-end">Moodboard</div>
        <div class="header-right hidden-small">
            <div class="text"></div>
            <button class="btn align-up hire">Welcome!!!!</button>
        </div>
    </div>
    <div class="middle">
        <div>
            <h2>Express your self with images</h2>
            <h3 class="middle-small">Zero Signup</h3>
        </div>
        <div class="middle-text">
            <div>Start a <a href="" class="newBoard">blank moodboard</a> or view the moodboards below:</div>
        </div>
    </div>
    <div>
       <div class="row">
            <div class="col-medium-4 col-small-6 flex-column">
                <img src="{{img}}" class="responsive-img index-img newBoard" style="cursor:pointer">
                <div>
                    Blank project
                </div>
            </div>
            {% for board in result %}
              <div class="col-medium-4 col-small-6">
              <a data-id = "{{board.id}}" class="board-button">
                  <img src="{{board.show_pic.src}}" class="responsive-img index-img">
                  </a>
                  <div style="text-align:center">
                      {{board.title}}
                  </div>
              </div>
            {% else %}
              <li>This would display if the 'item' collection were empty</li>
            {% endfor %}
       </div>
    </div>
</div>